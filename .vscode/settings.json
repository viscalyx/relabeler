{
    "terminal.integrated.shell.linux": "/bin/bash",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "always"
    },
    "editor.formatOnSave": true,
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "cSpell.ignorePaths": [
        ".git",
        "settings.json"
    ],
    "cSpell.words": [
        "Relabeler",
        "relabeler",
        "unmock",
        "viscalyx",
        "Viscalyx",
        "dbaeumer",
        "streetsidesoftware",
        "codecov",
        "davidanson",
        "firsttris",
        "gruntfuggly",
        "orta",
        "redhat",
        "usernamehw",
        "bierner",
        "azuretools",
        "errorlens"
    ],
    "extensions.ignoreRecommendations": true
}
