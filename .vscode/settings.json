{
    "terminal.integrated.shell.linux": "/bin/bash",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "always"
    },
    "editor.formatOnSave": true,
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "cSpell.ignorePaths": [
        ".git",
        "settings.json"
    ],
    "cSpell.words": [
        "Relabeler",
        "relabeler",
        "unmock"
    ],
    "extensions.ignoreRecommendations": true
}
